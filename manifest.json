{
  "manifest_version": 3,
  "name": "Phát hiện URL độc hại",
  "version": "1.0",
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "scripting",
    "webNavigation",
    "*://*/*"
  ],    
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
