{
  "name": "DetectURL",
  "description": "Phát hiện đường dẫn độc hại",
  "version": "1.1",
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "scripting",
    "webNavigation",
    "notifications",
    "*://*/*"
  ], 
  "background": {
    "service_worker": "service-worker.js"
  },
  "omnibox": {
    "keyword": "DetectURL"
  },
  "manifest_version": 3
}